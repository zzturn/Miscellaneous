      "manipulators": [
        { "description": "Hyper + Command + Q = Settings (if in Settings)", "type": "basic", "from": { "key_code": "q", "modifiers": { "mandatory": ["command"] } }, "to": [ { "key_code": "h", "modifiers": ["left_command"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 }, { "type": "frontmost_application_if", "bundle_identifiers": ["com.apple.systempreferences"] } ] },
