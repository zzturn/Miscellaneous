    "zzturn"
  ],
  "author": "zzturn",
  "json_url": "https://github.com/zzturn/Miscellaneous/raw/master/Karabiner/capslock.json",
  "import_url": "karabiner://karabiner/assets/complex_modifications/import?url=https://github.com/zzturn/Miscellaneous/raw/master/Karabiner/capslock.json",
  "repo": "https://github.com/zzturn/Miscellaneous/Karabiner",
  "rules": [
    {
      "description": " Hyper: Navigation, selection, and deletion: 激活 ",
      "manipulators":[
        { 
          "description": "CapsLock 作为 Hyper 层(按下激活, 单击为 Escape)", 
          "type": "basic", 
          "from": { "key_code": "caps_lock", "modifiers": { "optional": ["any"] } }, 
          "to": [ { "set_variable": { "name": "hyper_on", "value": 1, "key_up_value": 0 } } ], 
          "to_if_alone": [ { "key_code": "escape" } ]
        }
      ]
    },
    {
      "description": " Hyper: Navigation, selection, and deletion: 基本导航(方向键/行首行尾/分页) ",
      "manipulators":[
        { "description": "Hyper + H: 向左移动(iTerm2/Terminal为Ctrl+B, 其它为Left Arrow)", "type": "basic", "from": { "key_code": "h" }, "to": [ { "key_code": "b", "modifiers": ["left_control"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 }, { "type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm", "com.apple.Terminal"] } ] },
        { "description": "Hyper + H: 向左移动(其它应用Left Arrow)", "type": "basic", "from": { "key_code": "h" }, "to": [ { "key_code": "left_arrow" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + L: 向右移动(iTerm2/Terminal为Ctrl+F, 其它为Right Arrow)", "type": "basic", "from": { "key_code": "l" }, "to": [ { "key_code": "f", "modifiers": ["left_control"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 }, { "type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm", "com.apple.Terminal"] } ] },
        { "description": "Hyper + L: 向右移动(其它应用Right Arrow)", "type": "basic", "from": { "key_code": "l" }, "to": [ { "key_code": "right_arrow" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + J: 向下移动(Down Arrow)", "type": "basic", "from": { "key_code": "j" }, "to": [ { "key_code": "down_arrow" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + K: 向上移动(Up Arrow)", "type": "basic", "from": { "key_code": "k" }, "to": [ { "key_code": "up_arrow" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + I: 移动到行首(iTerm2/Terminal为Ctrl+A, 其它为Cmd+Left)", "type": "basic", "from": { "key_code": "i" }, "to": [ { "key_code": "a", "modifiers": ["left_control"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 }, { "type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm", "com.apple.Terminal"] } ] },
        { "description": "Hyper + I: 移动到行首(其它应用Cmd+Left)", "type": "basic", "from": { "key_code": "i" }, "to": [ { "key_code": "left_arrow", "modifiers": ["left_command"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + O: 移动到行尾(iTerm2/Terminal为Ctrl+E, 其它为Cmd+Right)", "type": "basic", "from": { "key_code": "o" }, "to": [ { "key_code": "e", "modifiers": ["left_control"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 }, { "type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm", "com.apple.Terminal"] } ] },
        { "description": "Hyper + O: 移动到行尾(其它应用Cmd+Right)", "type": "basic", "from": { "key_code": "o" }, "to": [ { "key_code": "right_arrow", "modifiers": ["left_command"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + U: 向上翻页(Page Up)", "type": "basic", "from": { "key_code": "u" }, "to": [ { "key_code": "page_up" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + P: 向下翻页(Page Down)", "type": "basic", "from": { "key_code": "p" }, "to": [ { "key_code": "page_down" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] }
      ]
    },
    {
      "description": " Hyper: Navigation, selection, and deletion: 按词/段落/整行导航 ",
      "manipulators":[
        { "description": "Hyper + Option + H: 按词向左移动(iTerm2/Terminal为Option+B, 其它为Option+Left)", "type": "basic", "from": { "key_code": "h", "modifiers": { "mandatory": ["option"] } }, "to": [ { "key_code": "b", "modifiers": ["left_option"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 }, { "type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm", "com.apple.Terminal"] } ] },
        { "description": "Hyper + Option + H: 按词向左移动(其它应用Option+Left)", "type": "basic", "from": { "key_code": "h", "modifiers": { "mandatory": ["option"] } }, "to": [ { "key_code": "left_arrow", "modifiers": ["left_option"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Option + L: 按词向右移动(iTerm2/Terminal为Option+F, 其它为Option+Right)", "type": "basic", "from": { "key_code": "l", "modifiers": { "mandatory": ["option"] } }, "to": [ { "key_code": "f", "modifiers": ["left_option"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 }, { "type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm", "com.apple.Terminal"] } ] },
        { "description": "Hyper + Option + L: 按词向右移动(其它应用Option+Right)", "type": "basic", "from": { "key_code": "l", "modifiers": { "mandatory": ["option"] } }, "to": [ { "key_code": "right_arrow", "modifiers": ["left_option"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Option + J: 向下快速移动(Down x3)", "type": "basic", "from": { "key_code": "j", "modifiers": { "mandatory": ["option"] } }, "to": [ { "key_code": "down_arrow" }, { "key_code": "down_arrow" }, { "key_code": "down_arrow" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Option + K: 向上快速移动(Up x3)", "type": "basic", "from": { "key_code": "k", "modifiers": { "mandatory": ["option"] } }, "to": [ { "key_code": "up_arrow" }, { "key_code": "up_arrow" }, { "key_code": "up_arrow" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] }
      ]
    },
    {
      "description": " Hyper: Navigation, selection, and deletion: 选中/选择 ",
      "manipulators":[
        { "description": "Hyper + Shift + H: 向左选中(Shift+Left)", "type": "basic", "from": { "key_code": "h", "modifiers": { "mandatory": ["shift"] } }, "to": [ { "key_code": "left_arrow", "modifiers": ["left_shift"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Shift + L: 向右选中(Shift+Right)", "type": "basic", "from": { "key_code": "l", "modifiers": { "mandatory": ["shift"] } }, "to": [ { "key_code": "right_arrow", "modifiers": ["left_shift"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Shift + J: 向下选中(Shift+Down)", "type": "basic", "from": { "key_code": "j", "modifiers": { "mandatory": ["shift"] } }, "to": [ { "key_code": "down_arrow", "modifiers": ["left_shift"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Shift + K: 向上选中(Shift+Up)", "type": "basic", "from": { "key_code": "k", "modifiers": { "mandatory": ["shift"] } }, "to": [ { "key_code": "up_arrow", "modifiers": ["left_shift"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + shift + Option + H: 按词向左选中(Option+Shift+Left)", "type": "basic", "from": { "key_code": "h", "modifiers": { "mandatory": ["shift", "option"] } }, "to": [ { "key_code": "left_arrow", "modifiers": ["left_option", "left_shift"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + shift + Option + L: 按词向右选中(Option+Shift+Right)", "type": "basic", "from": { "key_code": "l", "modifiers": { "mandatory": ["shift", "option"] } }, "to": [ { "key_code": "right_arrow", "modifiers": ["left_option", "left_shift"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + shift + Option + J: 向下选中三行(Shift+Down x3)", "type": "basic", "from": { "key_code": "j", "modifiers": { "mandatory": ["shift", "option"] } }, "to": [ { "key_code": "down_arrow", "modifiers": ["left_shift"] }, { "key_code": "down_arrow", "modifiers": ["left_shift"] }, { "key_code": "down_arrow", "modifiers": ["left_shift"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + shift + Option + K: 向上选中三行(Shift+Up x3)", "type": "basic", "from": { "key_code": "k", "modifiers": { "mandatory": ["shift", "option"] } }, "to": [ { "key_code": "up_arrow", "modifiers": ["left_shift"] }, { "key_code": "up_arrow", "modifiers": ["left_shift"] }, { "key_code": "up_arrow", "modifiers": ["left_shift"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] }
      ]
    },
    {
      "description": " Hyper: Navigation, selection, and deletion: 删除 ",
      "manipulators":[
        { "description": "Hyper + M: 删除前一个字符(Delete)", "type": "basic", "from": { "key_code": "m" }, "to": [ { "key_code": "delete_or_backspace" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Comma: 删除后一个字符(Delete Forward)", "type": "basic", "from": { "key_code": "comma" }, "to": [ { "key_code": "delete_forward" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + N: 删除前一个词(iTerm2/Terminal为Ctrl+W, 其它为Option+Delete)", "type": "basic", "from": { "key_code": "n" }, "to": [ { "key_code": "w", "modifiers": ["left_control"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 }, { "type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm", "com.apple.Terminal"] } ] },
        { "description": "Hyper + N: 删除前一个词(其它应用Option+Delete)", "type": "basic", "from": { "key_code": "n" }, "to": [ { "key_code": "delete_or_backspace", "modifiers": ["left_option"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Period: 删除后一个词(iTerm2/Terminal为Option+D, 其它为Option+Delete Forward)", "type": "basic", "from": { "key_code": "period" }, "to": [ { "key_code": "d", "modifiers": ["left_option"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 }, { "type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm", "com.apple.Terminal"] } ] },
        { "description": "Hyper + Period: 删除后一个词(其它应用Option+Delete Forward)", "type": "basic", "from": { "key_code": "period" }, "to": [ { "key_code": "delete_forward", "modifiers": ["left_option"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Command + N: 删除行内光标前面所有内容(iTerm2/Terminal为Control+u, 其他为Shift+Cmd+Left, Delete)", "type": "basic", "from": { "key_code": "n", "modifiers": { "mandatory": ["command"] } }, "to": [ { "key_code": "u", "modifiers": ["left_control"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 }, { "type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm", "com.apple.Terminal"] } ] },
        { "description": "Hyper + Command + N: 删除行内光标前面所有内容(Shift+Cmd+Left, Delete)", "type": "basic", "from": { "key_code": "n", "modifiers": { "mandatory": ["command"] } }, "to": [ { "key_code": "left_arrow", "modifiers": ["left_shift", "left_command"] }, { "key_code": "delete_or_backspace" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Command + Period: 删除行内光标后面所有内容(iTerm2/Terminal为Control+k, 其他为Shift+Cmd+Right, Delete)", "type": "basic", "from": { "key_code": "period", "modifiers": { "mandatory": ["command"] } }, "to": [ { "key_code": "k", "modifiers": ["left_control"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 }, { "type": "frontmost_application_if", "bundle_identifiers": ["com.googlecode.iterm", "com.apple.Terminal"] } ] },
        { "description": "Hyper + Command + Period: 删除行内光标后面所有内容(Shift+Cmd+Right, Delete)", "type": "basic", "from": { "key_code": "period", "modifiers": { "mandatory": ["command"] } }, "to": [ { "key_code": "right_arrow", "modifiers": ["left_shift", "left_command"] }, { "key_code": "delete_or_backspace" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] }
      ]
    },
    {
      "description": " Hyper: Navigation, selection, and deletion: 切换输入法/表情符号/CapsLock ",
      "manipulators":[
        { "description": "Hyper + Space: 切换输入法(Ctrl+Space)", "type": "basic", "from": { "key_code": "spacebar" }, "to": [ { "key_code": "spacebar", "modifiers": ["left_control"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Command + Space: 呼出表情符号(Ctrl+Cmd+Space)", "type": "basic", "from": { "key_code": "spacebar", "modifiers": { "mandatory": ["command"] } }, "to": [ { "key_code": "spacebar", "modifiers": ["left_control", "left_command"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Escape: 切换CapsLock(Ctrl+CapsLock)", "type": "basic", "from": { "key_code": "escape", "modifiers": {"optional": ["caps_lock"]} }, "to": [ { "key_code": "caps_lock", "modifiers": ["left_control"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] }
      ]
    },
    {
      "description": " Hyper: Window: 窗口切换 ",
      "manipulators": [
        { "description": "Hyper + A = Show/Hide Dock", "type": "basic", "from": { "key_code": "a" }, "to": [ { "key_code": "d", "modifiers": ["left_option", "left_command"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Command + A = Expose All", "type": "basic", "from": { "key_code": "a", "modifiers": { "mandatory": ["command"] } }, "to": [ { "key_code": "up_arrow", "modifiers": ["left_control"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Option + A = Show Desktop", "type": "basic", "from": { "key_code": "a", "modifiers": { "mandatory": ["option"] } }, "to": [ { "key_code": "f11" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Control + A = Launchpad", "type": "basic", "from": { "key_code": "a", "modifiers": { "mandatory": ["control"] } }, "to": [ { "shell_command": "open -a 'Launchpad'" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Shift + A = Toggle Notification Center", "type": "basic", "from": { "key_code": "a", "modifiers": { "mandatory": ["shift"] } }, "to": [ { "key_code": "n", "modifiers": ["fn"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },

        { "description": "Hyper + S = Tab Sheet", "type": "basic", "from": { "key_code": "s" }, "to": [ { "key_code": "tab", "modifiers": ["left_control"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] },
        { "description": "Hyper + Command + S = Tab Sheet Rev", "type": "basic", "from": { "key_code": "s", "modifiers": { "mandatory": ["command"] } }, "to": [ { "key_code": "tab", "modifiers": ["left_control", "left_shift"] } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 } ] }
      ]
    },
    {
