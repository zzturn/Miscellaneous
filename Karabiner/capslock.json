        { "description": "Hyper + Q = Activity Monitor (otherwise)", "type": "basic", "from": { "key_code": "q" }, "to": [ { "shell_command": "open -a 'Activity Monitor'" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 }, { "type": "frontmost_application_unless", "bundle_identifiers": ["com.apple.ActivityMonitor"] } ] },

        { "description": "Hyper + W = WeChat (otherwise)", "type": "basic", "from": { "key_code": "w" }, "to": [ { "shell_command": "open -a WeChat" } ], "conditions": [ { "type": "variable_if", "name": "hyper_on", "value": 1 }, { "type": "frontmost_application_unless", "bundle_identifiers": ["com.tencent.xinWeChat"] } ] },
