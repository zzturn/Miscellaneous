{
    "title": "Mouse tilt shortcuts",
    "rules": [
        {
            "description": "Custom: Map f18 to ctrl+left, f19 to ctrl+right (目前是鼠标中键向左是f18向右是f19)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f18",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_arrow",
                            "modifiers": [
                                "left_control"
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f19",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_arrow",
                            "modifiers": [
                                "left_control"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
